	cache_access_r(rc, UPDATE, addr, miss_counter, wb_counter, rc);
cache_full(c, idx, &pos)
	cache_update(c, idx, pos, REPLACE, tag)
cache_access_r(c->child, UPDATE, addr, miss_counter, wb_counter, rc);
cache_full(c, idx, &pos)
